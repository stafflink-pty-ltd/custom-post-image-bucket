{
    "name": "stafflink-pty-ltd/custom-post-image-bucket",
    "description": "Uploads images from XML data when imported using WP All Import",
    "type": "wordpress-plugin",
    "require": {
        "aws/aws-sdk-php": "^3.247",
        "composer/installers": "~1.0 || ^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CustomPostImageBucket\\": "src/"
        }
    },
    "config": {
        "platform-check": false,
        "vendor-dir": "lib",
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "authors": [
        {
            "name": "Matthew Neal",
            "email": "matt.neal@stafflink.com.au"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7"
    }
}
